<h1>Login</h1>

<%= form_with(scope: :session, url: login_path, local: true) do |f| %>
    <%= f.label :email %>
    <br/>
    <%= f.text_field :email, placeholder: "Enter email" %>
  </p>
  <p>
    <%= f.label :password %>
    <br/>
    <%= f.password_field :password, placeholder: "Enter password" %>
  </p>
  <p>
    <%= f.submit "Log in" %>
  </p>
<% end %>